{
  "name": "typescript-tutorial",
  "version": "1.0.0",
  "description": "Basic typescript tutorial for Bravo Team",
  "main": "index.js",
  "scripts": {
    "dev": "npm run build:live",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
